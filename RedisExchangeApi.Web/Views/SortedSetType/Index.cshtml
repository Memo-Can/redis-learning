@model HashSet<string>
@{
    ViewData["Title"] = "Sorte Set Type Samples";
}

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" placeholder="Name"/>
    <input type="number" name="score" placeholder="Score"/>
    <button type="submit">Save</button>
</form>
<br>
@foreach (var item in Model){
    <h1>@item <a asp-action="Delete" asp-route-name="@item">delete</a></h1>
}
